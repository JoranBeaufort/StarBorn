{% extends 'base.html.twig' %}
{% block css %}
    {{ parent() }}
    <style>
        .col-md-12{
            padding:0;
        }


        .main-content {
            padding-bottom:60px;
        }

    </style>
{% endblock css %}
    {% block content %}
        {% if app.request.hasPreviousSession %}
            {% for type, messages in app.session.flashbag.all() %}
                {% for message in messages %}
                    <div class="flash-{{ type }}">
                        {{ message }}
                    </div>
                {% endfor %}
            {% endfor %}
        {% endif %}


            {% block neo4j_user_content %}
            {% endblock neo4j_user_content %}

{% endblock content %}
{% block javascript %}
    {{ parent() }} 
{% endblock %}