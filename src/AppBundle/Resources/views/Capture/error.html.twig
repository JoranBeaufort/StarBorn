{% extends 'base.html.twig' %}
{% block title %}Dashboard{% endblock %} 
    {% block content %}
    
    <div class="container">
        <!--Card-->
        <div class="card">
            <div class="row">
                <div class="card-block">     
                    <div class="col-md-12">
                        <h3 class="card-title">Gebiet konnte nicht eingenommen werden!</h3>
                        {% if error != null %}
                        <div class="alert alert-danger">
                          <strong>Achtung!</strong> {{ error }}.
                        </div>
                        {% endif %}
                        <div class="alert alert-info">
                          <strong>Achtung!</strong> {{ info }}.
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
   <!--/.Mask-->
    {% endblock content %}

